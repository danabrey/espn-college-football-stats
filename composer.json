{
  "name": "danabrey/espn-college-football-stats",
  "description": "PHP package for reading data from ESPN's college football JSON API",
  "type": "library",
  "version": "2.1",
  "license": "MIT",
  "authors": [
    {
      "name": "Dan Abrey",
      "email": "danabrey@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "symfony/serializer": "^5.2",
    "symfony/property-access": "^5.2",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
